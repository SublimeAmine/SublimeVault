{
    "name": "sublimeskinz/sublime-vault",
    "description": "Get env params from Vault",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "~6.1",
        "aws/aws-sdk-php": "^3.52",
        "vlucas/phpdotenv": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "classmap": [
            "src"
        ],
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "Vendor\\SublimeSkinz\\SublimeVault\\": "/src"
        }
    },
    "license": [
        "SublimeSkinz"
    ]
}
